    android:layout_width="match_parent"
    android:layout_height="270dp"
    android:orientation="vertical"
    tools:context="com.android.quieromas.fragment.NutritionPlanRecipesFragment">

    <FrameLayout
        android:id="@+id/recipe_lunch_container"
        android:layout_width="match_parent"
        android:layout_marginBottom="3dp"
        android:layout_height="132dp"/>

    <FrameLayout
        android:id="@+id/recipe_dinner_container"
        android:layout_width="match_parent"
        android:layout_marginBottom="3dp"
        android:layout_height="132dp"/>

</LinearLayout>
